generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 Int     @id @default(autoincrement())
  login              String  @unique
  words              Word[]
  hashedPassword     String
  hashedRefreshToken String?
}

model Word {
  id     Int    @id @default(autoincrement())
  name   String
  User   User?  @relation(fields: [userId], references: [id])
  userId Int?
}
